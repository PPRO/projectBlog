@using MafieBlog.Class
<!DOCTYPE
 html>
<html lang="cs">
<head>
	<meta charset="utf-8">
	<!--[if IE]><meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"><![endif]-->
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta name="author" content="David Kováč">

	<meta name="description" content="">
	<meta name="keywords" content="inPage,blog,webové stránky,webhosting,rady,tipy,SEO,CSS,redakční systém, vlastní web, správa webu">
	<meta name="robots" content="all">
	<title>Title stránky</title>


	<link type="text/css" rel="stylesheet" media="screen,projection,tv" href="@Url.Content("~/Content/font-awesome.css")">
	<link type="text/css" rel="stylesheet" media="screen,projection,tv" href="@Url.Content("~/Content/main.css")">
	<link type="text/css" rel="stylesheet" media="screen,projection,tv" href="@Url.Content("~/Content/main2.css")">
	<link type="text/css" rel="stylesheet" media="screen,projection,tv" href="@Url.Content("~/Content/blueimp-gallery.min.css")">
	<link type="text/css" rel="stylesheet" media="screen,projection,tv" href="@Url.Content("~/Content/bootstrap-custom.css")">
	<link rel="stylesheet" type="text/css" media="screen,projection,tv" href="@Url.Content("~/Content/1.css")">
	<link rel="stylesheet" type="text/css" media="print" href="@Url.Content("~/Content/2.css")">
	<link rel="stylesheet" type="text/css" media="screen,projection,tv" href="@Url.Content("~/Content/3.css")">
	<link rel="stylesheet" type="text/css" media="screen,projection,tv" href="https://code.jquery.com/ui/1.8.24/themes/black-tie/jquery-ui.css">



	<script src="@Url.Content("~/Scripts/jquery-1.9.1.min.js")"></script>
	<script src="@Url.Content("~/Scripts/jquery.validate.min.js")"></script>
	<script src="https://code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
	<script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
	<script src="~/Scripts/MicrosoftAjax.js"></script>
	<script src="~/Scripts/MicrosoftMvcAjax.js"></script>
	<script src="~/Scripts/modernizr-2.6.2.js"></script>

	<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
	<!--[if lt IE 9]>
	  <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
	  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
	<![endif]-->

</head>

<body id="index">

	<header>
		<div class="container">
			<div class="row">
				<div class="col-xs-12 col-sm-3 col-md-4">
					<p id="logo">
						<a title="David Blog" href="/">
							<img src="@Url.Content("~/images/GAS_ICON.jpg")" alt="inPage - webové stránky a&nbsp;webhosting snadno a rychle">
						</a>
					</p>
				</div>
				<div class="col-xs-12 col-sm-9 col-md-8" id="topbar">
					<div class="row">
						<div class="col-sm-6" id="topcart">
							<div class="form-group">

							</div>
						</div>
						<div class="col-sm-6">
							<form id="form-search" role="search" method="post" action="/vyhledavani/">
								<div class="form-group">
									<div class="input-group">
										<input name="text" type="text" class="form-control" placeholder="Hledání...">
										<div class="input-group-btn">
											<button type="submit" class="btn btn-default" title="Hledat"><i class="fa fa-search"></i><span class="sr-only">Hledat</span></button>
										</div>
									</div>
								</div>
							</form>
						</div>
					</div>
				</div>
			</div>
		</div>
	</header>

	<nav class="navbar navbar-default navbar-static-top" role="navigation">
		<div class="container">

			<div class="navbar-collapse collapse">
				<ul id="list-navbar" class="nav navbar-nav level1 menu" role="menu">

					<li class="@Html.MenuLink( "Index", "Home")"><a href="@Url.Action("Index", "Home")">Úvodní strana</a></li>

					<li class="@Html.MenuLink( "Amerika", "Rubrika" )"><a href="@Url.Action( "Amerika", "Rubrika" )">Americká mafie</a></li>

					<li class="@Html.MenuLink( "Cesko", "Rubrika" )"><a href="@Url.Action( "Cesko", "Rubrika" )">Česká mafie</a></li>

					<li class="@Html.MenuLink( "Italie", "Rubrika" )"><a href="@Url.Action( "Italie", "Rubrika" )">Italská mafie</a></li>

					<li class="@Html.MenuLink( "Jakuza", "Rubrika" )"><a href="@Url.Action( "Jakuza", "Rubrika" )">Jakuza</a></li>

					<li class="@Html.MenuLink( "Rusko", "Rubrika" )"><a href="@Url.Action( "Rusko", "Rubrika")">Ruská mafie</a></li>

					<li class="@Html.MenuLink( "Zajimavosti", "Home" )"><a href="@Url.Action( "Zajimavosti", "Home" )">Zajímavosti</a></li>

					<li class="@Html.MenuLink( "Kontak", "Home" )"><a href="@Url.Action( "Kontakt", "Home" )">Kontakt</a></li>

				</ul>
			</div>
		</div>
	</nav>
	<!-- Articles-->
	<div id="content" class="container">
		<div class="row">

			@RenderBody()

			<aside class="col-xs-12 col-sm-12 col-md-4 col-md-pull-8 col-lg-3 col-lg-pull-9">
				<div id="panel-custom" class="panel panel-primary">
					<div class="panel-heading">
						<h3 class="panel-title">Odhlasit</h3>
					</div>
					@RenderSection("user", false)
					
				</div>

				<div id="panel-custom-1" class="panel panel-primary">
					<div class="panel-heading">
						<h3 class="panel-title">Tipy</h3>
					</div>
					<div class="panel-body">
						<ul id="list">
							@{Html.RenderAction( "Index", "MenuLeft" );}

						</ul>
					</div>
				</div>
			</aside>
			<div class="clear"></div>
			<!-- Start Footer Bottom -->
			<div id="footerbottom">

				<div class="footerwrap">

					<!-- Start Copyright Div -->
					<div id="copyright">&copy;2015. David Kovac</div>
					<!-- PLEASE SUPPORT US BY LEAVING THIS LINK -->
					<!-- End Copyright Div -->
					<!-- Start Social area -->
					<div class="socialfooter">

						<ul>
							<li><a href="#" class="social-google"></a></li>
							<li><a href="#" class="social-facebook"></a></li>
							<li><a href="#" class="social-twitter"></a></li>
							<li><a href="#" class="social-linkedin"></a></li>
							<li><a href="#" class="social-forrst"></a></li>
							<li><a href="#" class="social-dribbble"></a></li>
						</ul>

					</div>
					<!-- End Socialarea -->

				</div>

			</div>
			<!-- End Footer Bottom -->
		</div>
	</div> <!-- //container -->

	
	
	<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")"></script>
	<script src="@Url.Content("~/Scripts/jquery.blueimp-gallery.min.js")"></script>
	<script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
	<script src="@Url.Content("~/Scripts/bootstrap-image-gallery.min.js")"></script>
	<script src="@Url.Content("~/Scripts/inpage-main.js")"></script>
	<script src="@Url.Content("~/Scripts/tinymce/tinymce.min.js")"></script>
	<script>tinymce.init({ selector: '.formatedtext' });</script>

	<div id="imp"></div>
	<script type="text/javascript">
	var e = document.createElement("img");
	e.src = "/" + "render" + "/" + "imp" + "/";
	e.width = "1";
	e.height = "1";
	document.getElementById("imp").appendChild(e);
	</script>



</body>

</html>